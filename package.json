{
  "name": "custom-server-typescript",
  "version": "1.0.0",
  "engines": {
    "node": "12.4.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:3000"
    }
  },
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "heroku-postbuild": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server/index.js -p $PORT"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-session": "^2.0.37",
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.5.19",
    "@types/node": "^12.0.12",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth20": "^2.0.2",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "cookie-session": "^1.3.3",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "mongoose": "^5.7.3",
    "next": "latest",
    "nodemon": "^1.19.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "tailwindcss": "^1.1.2",
    "ts-node": "^8.1.0",
    "typescript": "3.6.3"
  }
}
