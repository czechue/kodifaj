    "@types/node": "^12.0.12",
    "@types/passport": "^1.0.1",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth20": "^2.0.2",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "cookie-session": "^1.3.3",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "final-form": "^4.18.6",
