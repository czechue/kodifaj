    "nodemon": "^1.19.0",
    "nuka-carousel": "^4.5.12",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "passport-google-oauth20": "^2.0.0",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-final-form": "^6.3.0",
